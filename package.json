{
    "name": "Atlantis-VIP",
    "displayName": "Atlantis VIP",
    "description": "Atlantis VIP - language for ERP Galaktika",
    "version": "0.0.5",
    "publisher": "Slonsoid",
    "license": "SEE LICENSE IN LICENSE",
    "pricing": "Free",
    "engines": {
        "vscode": "^1.60.0"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/Slonsoid/Atlantis-VIP"
    },
    "categories": [
        "Programming Languages"
    ],
    "icon": "images/icon.png",
    "contributes": {
        "languages": [{
            "id": "vip",
            "aliases": ["Atlantis VIP", "vip"],
            "extensions": [".vip",".prj",".vih",".inc",".frm",".vpp",".vil",".obj",".mnu",".gcd",".ccm",".csc",".tbl",".lih",".pan"],
            "configuration": "./language-configuration.json"
        }],
        "grammars": [{
            "language": "vip",
            "scopeName": "source.vip",
            "path": "./syntaxes/VIP.tmLanguage.json"
        }],
        "snippets": [{
            "language": "vip",
            "path": "./snippets/vip_snippets.json"
        }],
        "VIP": {
            "customData": [
                "./vip.vip-data.json"
            ]
        }
    }
}